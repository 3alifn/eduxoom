      dataType: "JSON",
      success: function (res) {
        $('.count-teacher').text(res.countTeacher);
        $('.percent-t-male').append(res.percentTmale);
        $('.percent-t-male').append(` (${res.tMale})`);
        $('.percent-t-female').append(res.percentTfemale);     
        $('.percent-t-female').append(`(${res.tFemale})`);     
        
