<script>



  $(".team-saanvi-form").on('submit', (event)=>{
   event.preventDefault();
   
   $.ajax({
      method: "post",
      url: "/pu/team-saanvi/contact-us",
      data: $(".team-saanvi-form").serialize(),
      dataType: "JSON",
      beforeSend: function(){
     $('.contactus').addClass('disabled')
          _spin_pull()
      },
      success: function (response) {
        $('.contactus').removeClass('disabled')
        _spin_pop()
        _msg_pull('alert-success', response.msg)

      }
    });
  })

  </script>
