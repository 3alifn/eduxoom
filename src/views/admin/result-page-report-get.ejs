          $('.bi-gp2').append(`<div class="flex-fill border border-dark ${res.gp02>index?'bg-secondary':''}"></div>`)
          
        }  for (let index = 0; index < 7; index++) {
          $('.bi-gp3').append(`<div class="flex-fill border border-dark ${res.gp03>index?'bg-secondary':''}"></div>`)
          
        }


      }
    });

  }

  bi_checkout_class()
  
  
