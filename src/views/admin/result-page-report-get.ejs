      dataType: "JSON",
      success: function (res) {
        for (let index = 0; index < 7; index++) {
          $(`.gp1_${res.subject_code}`).append(`<div class="flex-fill border border-dark ${res.gp1>index?'bg-secondary':''} "></div>`)
          
